body{  background-color: #e4e4e4;}.page-content{  width: 100%;  background-color: rgb(228,228,228);  .container-fluid{    padding: 0;  }  .content-header{    padding:0 32px;    background-color: #fff;    .nav-con{      font-size: 14px;      height: 54px;      line-height: 22px;      padding:16px 0;    }    .header-con{      height: 140px;      .hl{        h3{          font-size: 20px;          margin: 0;          height: 32px;          line-height: 32px;          padding-left:44px;          position: relative;          &:before{            content: '';            width: 28px;            height: 28px;            background-image: url("../img/u3359.png");            background-repeat: no-repeat;            background-size: 100% 100%;            position: absolute;            left: 0;            top:50%;            transform: translate(0,-50%);          }        }        .hl-con{          width: 500px;          margin-top: 4px;          .items{            margin-top: 18px;            .title,.text{              font-size: 14px;              height: 14px;              line-height: 14px;            }            .title{              color:rgba(102,102,102,.847);            }            .text{              color:rgba(51,51,51,.847);            }          }        }      }      .hr{        .order-state{          margin-top: 14px;          text-align: right;          .title{            color: rgba(0,0,0,.447);            font-size: 14px;            height: 14px;            line-height: 14px;          }          .state{            font-size: 20px;            height: 20px;            line-height: 20px;            margin-top: 14px;            color: rgba(0,0,0,.847);          }        }      }    }    .tab{      li{        float: left;        margin-right: 20px;        width: 89px;        height: 46px;        line-height: 46px;        text-align: center;        color: rgb(102,102,102);        font-size: 14px;        cursor: pointer;        &:hover{          border-bottom: 1px solid rgb(18,143,233);        }        a{          color: rgb(102,102,102);          text-decoration: none;        }      }      li.active{        a{          color:rgb(18,143,233);        }        border-bottom: 2px solid rgb(18,143,233);      }    }  }  .main{    padding:20px 30px;    >.panel-item{      display: none;    }    >.panel-item.active{      display: block;    }    .panel{      border-radius: 0;      border: 0;      box-shadow: none;      margin-bottom: 0;      .panel-heading{        background-color: #fff;        height: 55px;        padding:10px 34px;        h3{          margin: 0;          font-size: 16px;          color: #333;          line-height: 34px;        }      }      .panel-body{        padding:15px 34px;      }    }    .main-items{      .panel-handing{        padding:10px 0;        h3{          margin: 0;          font-size: 16px;        }      }      .panel-body{        padding:15px 0;        .row{          margin: 0;          .col{            padding: 6px 30px;          }          .disabled{            .fa{              color: #ccc;            }          }          label{            margin-bottom: 10px;            i{              color: red;              font-size: 12px;              margin-right: 4px;            }          }          .checkbox{            margin: 0;            padding:6px 0;            height: 34px;            label{              padding-left: 0;              margin-bottom: 0;              margin-right: 24px;              input{                margin-right: 8px;              };            }          }          .form-input{            position: relative;            &:after{              content: '';              position: absolute;              top:50%;              right:10px;              transform: translate(0,-50%);              width: 20px;              height: 20px;              background-image: url("../img/u3980.png");              background-repeat: no-repeat;              background-size: 100% 100%;            }          }          .border-none{            border: 0;            box-shadow: none;            padding-left: 0;          }          .form-textarea{            width: 100%;            overflow: auto;            word-break: break-all;            resize: none;          }        }        //内联表单样式        .form-inline{          label{            min-width: 90px;          }          .col{            padding: 0 30px;          }          .checkbox-break{            >label{              opacity: 0;            }          }          .checkbox{            label{              margin-right: 0;            }          }          .item-inline{            padding: 6px 30px;          }        }      }    }    //用户信息    .user-info{      padding: 10px 0 15px 0;      .head-portrait{        width: 80px;        height: 80px;        border-radius: 40px;        background-repeat: no-repeat;        background-position: center center;        background-size: cover;      }      .info{        font-size: 14px;        padding: 16px 0 0 30px;        p{          i,em{            font-style: normal;          }          i{            color: #333;          }          em{            color: #888;          }        }      }    }    .allmap{      width: 100%;      height: 400px;      margin: 10px 0 20px 0;      border: 1px solid #ccc;    }    //房屋图片    .house-pic-list{      width: calc(100% - 90px);      ul{        width: 100%;      }      li{        float: left;        margin-right: 20px;        margin-bottom: 10px;        border: 1px solid #e9e9e9;        .img-con{          .img{            width: 100px;            height: 100px;            background-repeat: no-repeat;            background-position: center center;            background-size: cover;          }          p{            font-size: 14px;            height: 34px;            line-height: 34px;            text-align: center;            margin: 0;          }        }      }    }    //热度标签    .hot-tag{      li{        float: left;        margin-right: 20px;        margin-bottom: 10px;        >div{          padding:5px 10px;          font-size: 14px;          line-height: 14px;          color: rgb(255, 153, 0);          border: 1px solid rgb(255, 153, 0);        }      }    }    //关联活动    .activity{      width: calc(100% - 90px);      p{        word-break: break-all;        line-height: 24px;      }    }    //操作纪录    .operating-discipline{      list-style: none;      li{        list-style: none;        margin-bottom: 10px;        p{          margin: 0;          span{            color: #108EE9;            padding: 0 5px;          }        }      }    }    //维修订单    .oredr-maintenance{      .star-list{        margin-left: -5px;        li{          float: left;          width: 44px;          text-align: center;          i{            font-size: 34px;            color: #808080;          }          p{            color: #999;          }          &:hover{            i{              color: #ffd301;            }            p{              color: #383838;            }          }        }        li.active{          i{            color: #ffd301;          }          p{            color: #383838;          }        }      }      .file{        height: 34px;        width: 120px;        text-align: center;        line-height: 32px;        border: 1px solid;        color: #fff;        background-color: #337ab7;        border-color: #2e6da4;        position: relative;        input{          opacity: 0;          width: 120px;          height: 34px;          position: absolute;          left: 0;          top:0;        }      }    }    .form-horizontal{      i.fa-asterisk{        color: red;        font-size: 12px;        margin-right: 4px;      }      .checkbox{        label{          min-width: 90px;        }      }      .checkbox-multiple-line{        label{          margin-bottom: 20px;        }      }    }  }}.housingInfo{  .content-header{    h3{      font-size: 28px;      padding: 0 0 22px 0;      margin: 0;    }  }  .pic{    border: 1px solid #ccc;    .pic-title{      border-bottom: 1px solid #ccc;      .tab-list{        padding-top: 10px;        li{          float: left;          width: 62px;          margin: 0 15px;          text-align: center;          height: 30px;          line-height: 28px;          color: #333;        }        li.active{          color: #1890FF;          border-bottom: 2px solid #1890FF;        }      }    }    .pic-con{      .pic-list{        padding:30px 15px 34px 15px;        min-height: 186px;        display: none;        li{          width: 96px;          height: 122px;          margin: 0 15px;          float: left;          .img-con{            width: 100%;            height: 92px;            position: relative;            overflow: hidden;            background-repeat: no-repeat;            background-position: center;            background-size: cover;            img{              display: block;              width: 100%;            }            .mask{              display: none;              position: absolute;              width: 100%;              height: 92px;              line-height: 92px;              text-align: center;              left: 0;              top:0;              background-color: rgba(0,0,0,.4);              z-index: 2;              color: #f0f0f0;            }          }          .del{            display: none;            width: 100%;            height: 30px;            line-height: 30px;            text-align: center;            background-color: #f0f0f0;            color: #909090;          }          &:hover{            cursor: pointer;            border: 1px solid #e9e9e9;            .mask,.del{              display: block;            }          }        }      }      .pic-list0{        display: block;      }    }    .pic-file{      height: 50px;      border-top: 1px solid #ccc;      padding: 9px;      position: relative;      input{        position: absolute;        left: 9px;        top: 9px;        opacity: 0;        width: 120px;        height: 32px;        z-index: 2;        outline: none;      }      .file-btn{        width: 120px;        text-align: center;        border: 1px solid #2e6da4;        color: #fff;        background-color: #337ab7;        outline: none;      }      .flie-num{        line-height: 34px;        margin-left: 10px;      }    }  }  .file-video{    .file-btn{      margin-left: 10px;      margin-right: 10px;      outline: none;    }    input{      position: absolute;      left: 10px;      top:10px;      width: 120px;      height: 34px;      opacity: 0;      z-index: 2;      outline: none;      margin-right: 9px;    }  }  .hot-tag{    padding-top: 5px;    li{      float: left;      margin-right: 20px;      margin-bottom: 10px;      padding: 5px 10px;      font-size: 14px;      line-height: 14px;      color: #999;      border: 1px solid rgba(228, 228, 228, 1);      cursor: pointer;    }    .active{      color: #ff9900;      border: 1px solid #ff9900;    }  }  .select-items{    .select-item-{      position: relative;      .help-block{        position: absolute;        padding: 0 20px;        right:-50px;        cursor: pointer;      }    }    .btn-add{      border-style: dashed;      outline: none;    }  }  .roommates{    display: none;  }}.statistical{  .btn-search{    margin-right: 20px;    color: #808080;    li{      float: left;      line-height: 34px;      margin: 0 10px;    }    .active{      color: #1890FF;    }  }  .statistical-con{    width: 100%;    min-height: 500px;    padding-bottom: 20px;  }  .statistical-title{    position: relative;    z-index: 33;    margin-top: -4px;    float: right;    li{      float: left;      min-width: 150px;      text-align: right;      padding-bottom: 30px;      border-top:2px solid #fff;      p{        padding-top: 32px;        padding-bottom: 15px;        font-size: 15px;        color: #999;      }      h3{        font-size: 24px;        color: #333;        margin:0;      }    }    .active{      border-top:2px solid  #1890FF;      p{        color: #1890FF;      }      h3{        color: #1890FF;      }    }  }}.coupon{  .panel-heading{    padding-right: 0;    padding-left: 0;  }  h3{    margin: 0;    line-height: 34px;  }}.coupon-details{  .filter-con{    display: none;  }}.preview-mask{  position: fixed;  left:0;  right:0;  top:0;  bottom: 0;  z-index: 100;  .mask{    position: absolute;    left:0;    right:0;    top:0;    bottom: 0;    z-index: 101;    background-color: rgba(0,0,0,.3);  }  .preview-con{    position: absolute;    right:20px;    top:50%;    z-index: 102;    width: 416px;    transform: scale(.8) translateY(-62.5%);    background-color: #fff;    .preview-title{      width: 100%;      padding: 0 20px;      height: 64px;      line-height: 64px;      border-bottom: 1px solid #f5f5f5;      background-color: #fff;    }    .preview-main{      margin: 20px;      width: 375px;      height: 667px;      background-color: #f5f5f5;      overflow-y: auto;      ::-webkit-scrollbar{        display:none;        width: 0;      }      .phone-main{        width: 100%;        background-color: #f5f5f5;        padding: 15px;        .banner{          width: 100%;          height:180px;          background-color: #fff;          text-align: center;          padding-top: 50px;          background-image: url("../img/pic.jpg");          background-position: center;          background-size: cover;          color: #fff;        }        .rule{          background-color: #fff;          width: 100%;          padding: 15px;          margin-top: 10px;          h3{            text-align: center;            font-size: 16px;            margin-top: 0;            margin-bottom: 15px;          }          p{            font-size: 14px;            word-break: break-all;          }        }      }    }  }}